<p id="notice"><%= notice %></p>

<h1>Beneficiarios</h1>

<table>
  <thead>
    <tr>
      <th>Tipo cedula</th>
      <th>Cedula</th>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Sexo</th>
      <th>Fecha de nacimiento</th>
      <th>Oficio</th>
      <th>Estado civil</th>
      <th>Tipo de casa</th>
      <th>Num habitaciones</th>
      <th>Num banos</th>
      <th>Enseres</th>
      <th>Obs enseres</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @beneficiarios.each do |beneficiario| %>
      <tr>
        <td><%= beneficiario.tipo_cedula %></td>
        <td><%= beneficiario.cedula %></td>
        <td><%= beneficiario.nombres %></td>
        <td><%= beneficiario.apellidos %></td>
        <td><%= beneficiario.sexo %></td>
        <td><%= beneficiario.fecha_de_nacimiento %></td>
        <td><%= beneficiario.oficio %></td>
        <td><%= beneficiario.estado_civil %></td>
        <td><%= beneficiario.tipo_de_casa %></td>
        <td><%= beneficiario.num_habitaciones %></td>
        <td><%= beneficiario.num_banos %></td>
        <td><%= beneficiario.enseres %></td>
        <td><%= beneficiario.obs_enseres %></td>
        <td><%= link_to 'Show', beneficiario %></td>
        <td><%= link_to 'Edit', edit_beneficiario_path(beneficiario) %></td>
        <td><%= link_to 'Destroy', beneficiario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Beneficiario', new_beneficiario_path %>
