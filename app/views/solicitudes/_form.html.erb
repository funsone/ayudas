<%= simple_form_for([@solicitante, @beneficiario, @solicitud]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :status %>
    <%= f.input :descripcion %>
    <%= f.association :ayuda %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
